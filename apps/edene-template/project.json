{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/edene-template/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/web:webpack",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "outputPath": "dist/apps/edene-template",
                "index": "apps/edene-template/src/index.html",
                "baseHref": "/",
                "main": "apps/edene-template/src/main.tsx",
                "polyfills": "apps/edene-template/src/polyfills.ts",
                "tsConfig": "apps/edene-template/tsconfig.app.json",
                "assets": [
                    "apps/edene-template/src/favicon.ico",
                    "apps/edene-template/src/assets",
                    "apps/edene-template/src/netlify.toml"
                ],
                "styles": [],
                "scripts": [],
                "webpackConfig": "@nrwl/react/plugins/webpack"
            },
            "configurations": {
                "production": {
                    "fileReplacements": [{
                        "replace": "apps/edene-template/src/environments/environment.ts",
                        "with": "apps/edene-template/src/environments/environment.prod.ts"
                    }],
                    "optimization": true,
                    "outputHashing": "all",
                    "sourceMap": false,
                    "namedChunks": false,
                    "extractLicenses": true,
                    "vendorChunk": false
                },
                "development": {
                    "extractLicenses": false,
                    "optimization": false,
                    "sourceMap": true,
                    "vendorChunk": true
                }
            }
        },
        "serve": {
            "executor": "@nrwl/web:dev-server",
            "options": {
                "buildTarget": "edene-template:build",
                "hmr": true,
                "port": 4201
            },
            "configurations": {
                "production": {
                    "buildTarget": "edene-template:build:production",
                    "hmr": false
                },
                "development": {
                    "buildTarget": "edene-template:build:development"
                }
            },
            "defaultConfiguration": "development"
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/edene-template/**/*.{ts,tsx,js,jsx}"]
            }
        },
        "deploy": {
            "executor": "@netlify-builder/deploy:deploy",
            "options": {
                "outputPath": "dist/apps/edene-template",
                "siteId": "cb556dad-569b-4128-8b6b-2de4a7ffc11e"
            }
        }
    },
    "tags": []
}
