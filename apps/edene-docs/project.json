{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/edene-docs",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/next:build",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "root": "apps/edene-docs",
                "outputPath": "dist/apps/edene-docs",
                "assets": [
                    "apps/edene-docs/netlify.toml"
                ]
            },
            "configurations": {
                "development": {},
                "production": {}
            }
        },
        "serve": {
            "executor": "@nrwl/next:server",
            "defaultConfiguration": "development",
            "options": {
                "buildTarget": "edene-docs:build",
                "dev": true
            },
            "configurations": {
                "development": {
                    "buildTarget": "edene-docs:build:development",
                    "dev": true
                },
                "production": {
                    "buildTarget": "edene-docs:build:production",
                    "dev": false
                }
            }
        },
        "export": {
            "executor": "@nrwl/next:export",
            "options": {
                "buildTarget": "edene-docs:build:production"
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/apps/edene-docs"],
            "options": {
                "jestConfig": "apps/edene-docs/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/edene-docs/**/*.{ts,tsx,js,jsx}"]
            }
        }
    },
    "tags": []
}
